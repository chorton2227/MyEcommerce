{
  "name": "myecommerce.services.shoppingcartservice.api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run gen:tsoa && tsc",
    "watch": "npm run gen:tsoa && tsc -w",
    "dev": "nodemon dist/index.js",
    "start": "node dist/index.js",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f ./ormconfig.json",
    "migrate:generate": "npm run typeorm:cli -- migration:generate -n",
    "migrate:up": "npm run typeorm:cli -- migration:run",
    "migrate:down": "npm run typeorm:cli -- migration:revert",
    "gen:env": "gen-env-types .env -o src/env.d.ts -e .",
    "gen:tsoa": "tsoa spec-and-routes"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.1",
    "tsoa": "^3.14.1",
    "typeorm": "^0.2.40"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.7",
    "gen-env-types": "^1.3.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
}
